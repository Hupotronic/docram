!!! 5
html
	head
		title= fname
		link(id="docram-style",rel="stylesheet",href=styleBase)
		link(id="docram-code",rel="stylesheet",href=styleCode)
	body
		#container
			#background
			table(cellpadding=0,cellspacing=0)
				thead
					tr
						th.docs
							h1= fname
						th.code
				tbody
					each section, i in sections
						tr(id="section-"+i)
							td.docs !{section.doc}
							td.code
								pre
									code !{section.code}
